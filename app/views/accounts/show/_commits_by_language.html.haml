- chart_url = commits_by_language_chart_account_url(@account, scope: 'regular').to_s

.margin_top_10
  .pull-left
    %h4= link_to t('.link'), languages_account_path(@account)
  .pull-left.soft.margin_top_10.margin_left_20= chart_default_time_span
  .clearfix
.stream_graph.regular#ohloh_streamgraph{ 'datascope' => 'regular', 'datasrc' => chart_url }
