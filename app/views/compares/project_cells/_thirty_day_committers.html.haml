- if !project.best_analysis.nil? && project.best_analysis.last_commit_time
  - tds = project.best_analysis.thirty_day_summary
  - if tds
    - if tds.committer_count > 0
      - str = pluralize_with_delimiter(tds.committer_count, t('compares.developer'))
      = link_to str, project_contributors_path(project, time_span: '30 days')
    - else
      %span.status.warning= t('no_activity')
  - else
    %span.soft= t('compares.no_data')
- elsif project.enlistments.count > 0
  %span.soft= t('compares.pending')
- else
  %span.soft= t('compares.no_data')
