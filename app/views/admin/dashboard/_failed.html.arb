slave.jobs.failed
  .where("current_step_at is null or current_step_at > '#{get_window}'")
  .order(:status, :id).limit(20)
  .each_with_index do |job, i|
    a href: admin_job_path(job), style:"text-decoration:none;" do
      status_tag("#{job.class.to_s.first} #{job.id}", 'failed', title: JobDecorator.new(job).tool_tip)
    end
    br if i.odd?
end
