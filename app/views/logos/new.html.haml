- if @project_or_organization.is_a?(Organization)
  - @meta_title = t('.meta_title')
- else
/  - @meta_title_final = project_pages_title(t('.page_title'))
.project_content_title
  %h2.float_left
    /= link_to "Settings", settings_url_for(@project_or_organization)
    = t('.heading')
  - unless @project_or_organization.is_a?(Organization)
    /- render_project_analysis_timestamp(@project_or_organization)

.row
  .col-md-10.col-md-offset-1
    .well
      = form_for [@project_or_organization, @logo], html: { multipart: true } do |f|
        = render partial: 'fields', locals: { f: f }
      .clear
        &nbsp;

- if @project_or_organization.is_a?(Project)
  = render partial: 'about_logos'
- elsif @project_or_organization.is_a?(Organization)
  = render partial: 'about_organization_logos'
