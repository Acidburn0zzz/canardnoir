:ruby
  col1 = 'Open Hub account'
  col2 = 'Application Name'
  col3 = 'URL'
  col4 = 'Description'
  col5 = 'Daily limit'
  col6 = 'Created'
  col7 = 'All time requests'
  col8 = 'Requests today'
  col9 = 'Last accessed'
  col10 = 'Status'
  col11 = 'API key'
= [col1, col2, col3, col4, col5, col6, col7, col8, col9, col10, col11].join ','
- @models.each do |key|
  :ruby
    status = (key.status == ApiKey::STATUS_OK) ? 'Active' : nil
    status ||= (key.status == ApiKey::STATUS_LIMIT_EXCEEDED) ? 'Limit Exceeded' : 'Disabled'
    a = key.account.name
    b = key.name
    c = key.url
    d = key.description
    e = key.daily_limit
    f = key.created_at.strftime('%m/%d/%Y')
    g = key.total_count
    h = key.daily_count
    i = key.last_access_at
    j = status
    k = key.key
  = [a, b, c, d, e, f, g, h, i, j, k].join ','
